        <TestCase testID="Test_pumb_Skin_over_swfsymbol_embeded" description="Skins set by skin states being validated." keywords="[SkinStates, PopUpMenuButtons, over]">
            <setup>
                <ResetComponent target="myPopUpButton_swfsymbol_embeded" className="mx.controls.PopUpButton" waitEvent="updateComplete" waitTarget="myPopUpButton_swfsymbol_embeded" />
                <SetProperty target="myPopUpButton_swfsymbol_embeded" propertyName="label" value="Check popUpOverSkin is displayed." waitTarget="myPopUpButton_swfsymbol_embeded" waitEvent="updateComplete"/>

                <SetProperty target="myPopUpButton_swfsymbol_embeded" propertyName="width" value="250" waitTarget="myPopUpButton_swfsymbol_embeded" waitEvent="updateComplete"/>
                <SetProperty target="myPopUpButton_swfsymbol_embeded" propertyName="height" value="23" waitTarget="myPopUpButton_swfsymbol_embeded" waitEvent="updateComplete" />
                    <DispatchMouseClickEvent localX="10" localY="10" target="myPopUpButton_swfsymbol_embeded" timeout="2000" waitTarget="myPopUpButton_swfsymbol_embeded" waitEvent="mouseUp"/>
                    <DispatchMouseEvent localX="10" localY="10" buttonDown="false" type="mouseOver" target="myPopUpButton_swfsymbol_embeded" timeout="1000" waitTarget="myPopUpButton_swfsymbol_embeded" waitEvent="mouseOver" />
            </setup>
            <body>
                    <AssertPixelValue target="myPopUpButton_swfsymbol_embeded" x="10" y="10" value="0xB5E0D9" />
            </body>
