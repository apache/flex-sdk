	<TestCase testID="LineXYStroke_transform_translation_setLayoutBoundsPosition" keywords="[transform, methods, layoutGraphicElement]" description="Call actualPosition after setting x and y via transform matrix">
        	<setup> 
           		<ResetComponent target="pan" className="comps.MyGroup" waitEvent="updateComplete" waitTarget="pan"/>
           		<RunCode code="application.pan.l2.setLayoutBoundsPosition(20, 30)"/>
        	</setup>
            	<body>
                	<AssertPropertyValue propertyName="x" target="pan.l2" value="20.5" />
                	<AssertPropertyValue propertyName="y" target="pan.l2" value="30.5" />
            	</body>
        </TestCase>
       
	<TestCase testID="LineXYStroke_transform_scalingX_matrix_setLayoutBoundsPosition" keywords="[transform, methods, layoutGraphicElement]" description="Call actualPosition after scaling the button via transform matrix">
		<setup> 
			<ResetComponent target="pan" className="comps.MyGroup" waitEvent="updateComplete" waitTarget="pan"/>
			<SetProperty propertyName="includeInLayout" value="false" target="pan.l2"/>
			<SetProperty propertyName="scaleX" target="pan.l2" value="5"/>
			<SetProperty propertyName="scaleY" target="pan.l2" value="1" waitEvent="updateComplete" waitTarget="pan"/>
			
			<RunCode code="application.pan.l2.setLayoutBoundsPosition(20,30)"/>
		</setup>
		<body>
			<AssertMethodValue method="value=application.pan.l2.getLayoutBoundsX()" value="20" />
			<AssertMethodValue method="value=application.pan.l2.getLayoutBoundsY().toFixed(0)" value="30" />
			<AssertMethodValue method="value=application.pan.l2.getLayoutBoundsX(false)" value="21.302776" />
			<AssertMethodValue method="value=application.pan.l2.getLayoutBoundsY(false)" value="31.302776" />
		</body>
        </TestCase>
