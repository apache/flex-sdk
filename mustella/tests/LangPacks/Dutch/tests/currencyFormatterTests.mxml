				<SetProperty propertyName="currencySymbol" target="mf.cf" value="NµHJk"/>
				<RunCode code="application.tin.text=application.mf.cf.format(98765);"/>
			</setup>
			<body>
				<AssertPropertyValue target="tin" propertyName="text" value="NµHJk98.765" />
			</body>
		</TestCase>
		
