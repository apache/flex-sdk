    private function triggerMissingCardNumberRTE():void{
        var ccv:CreditCardValidator = new CreditCardValidator();
        var obj:Object = new Object();
        obj.cardType=CreditCardValidatorCardType.MASTER_CARD;
        obj.cardNumber=null;
