		<TestCase testID="Spark_Checking_downIcon_jpg_of_Button" description="Test the downIcon jpg style of Button"  keywords="[Button, downIcon, style]">

			<setup>

				<ResetComponent target="myButton" className="mx.controls.Button" waitEvent="updateComplete"  waitTarget="myButton"/>

