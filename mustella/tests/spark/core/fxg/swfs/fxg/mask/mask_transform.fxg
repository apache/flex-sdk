<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008" viewHeight="770" viewWidth="830">
  	<Group x="20" y="30">
	  <Group x="20" y="90">
		<Group>
			<Group>
			    <Group x="30" y="80">
				<Group maskType="clip">
					<mask>					
						<Group x="0" y="0" scaleX="3" scaleY="2" rotation="-15">
							<Ellipse width="100" height="50">
								<fill>
									<RadialGradient x="50" y="10" scaleX="100" scaleY="60" focalPointRatio="0">
										<GradientEntry color="#0900b2" ratio="0"/>
										<GradientEntry color="#ff0000" ratio="0.5"/>
										<GradientEntry color="#fffc00" ratio="1"/>
									</RadialGradient>
								</fill>
								<stroke>
								  <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
								</stroke>
							</Ellipse>
							<Ellipse x="50" width="100" height="50">
								<fill>
									<LinearGradient x = "10" y = "10" scaleX = "50" rotation = "-17">
										<GradientEntry color="#203c00" ratio="0" alpha="1"/>
										<GradientEntry color="#1b971b" ratio="0.28" alpha="1"/>
										<GradientEntry color="#1e4f45" ratio="0.45" alpha="1"/>
										<GradientEntry color="#6aec01" ratio="0.6" alpha="1"/>
										<GradientEntry color="#1b971b" ratio="0.75" alpha="1"/>
										<GradientEntry color="#203c00" ratio="1" alpha="1"/>
									</LinearGradient>
								</fill>
							</Ellipse>
						</Group>
					</mask>	      		
					<Group x="0" y="0">
						<BitmapGraphic width="120" height="100" source="@Embed('../../../../../../../Assets/Images/spectrum.png')" repeat="false"/>
					</Group>
					<Group x="230" y="0">
						<Rect radiusX="20" radiusY="40" width="120" height="100">
							<fill>
								<LinearGradient x = "20" y = "20" scaleX = "50" rotation = "-17">
									<GradientEntry color="#0900b2" ratio="0"/>
									<GradientEntry color="#ff0000" ratio="0.5"/>
									<GradientEntry color="#fffc00" ratio="1"/>
								</LinearGradient>
							</fill>
						</Rect>
					</Group>
				</Group>
				<Group maskType="alpha">
					<mask>	
						<Group alpha="0.8">
							<Ellipse width="100" height="50" x="60" y="200" scaleX="3" scaleY="2" rotation="-15">
								<fill>
									<LinearGradient x = "10" y = "10" scaleX = "50" rotation = "-17">
										<GradientEntry color="#203c00" ratio="0" alpha="1"/>
										<GradientEntry color="#1b971b" ratio="0.28" alpha="1"/>
										<GradientEntry color="#1e4f45" ratio="0.45" alpha="1"/>
										<GradientEntry color="#6aec01" ratio="0.6" alpha="1"/>
										<GradientEntry color="#1b971b" ratio="0.75" alpha="1"/>
										<GradientEntry color="#203c00" ratio="1" alpha="1"/>
									</LinearGradient>
								</fill>
								<stroke>
								  <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
								</stroke>
							</Ellipse>
						</Group>
					</mask>
					<Group x="0" y="150">
						<Ellipse width="120" height="80" alpha="0.5">
							<filters>
								<BlurFilter blurX="4" blurY="1" quality="3"/>
							</filters>
							<fill>
								<RadialGradient x="50" y="10" scaleX="100" scaleY="60" focalPointRatio="0">
									<GradientEntry color="#0900b2" ratio="0"/>
									<GradientEntry color="#ff0000" ratio="0.5"/>
									<GradientEntry color="#fffc00" ratio="1"/>
								</RadialGradient>
							</fill>
							<stroke>
							  <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
							</stroke>
						</Ellipse>
					</Group>
					<Group x="230" y="150">
						<Rect x="10" y="10" width="120" height="120" alpha="0.5">
							<filters>
								<BevelFilter distance="15" highlightAlpha="0.5" highlightColor="#228B22" shadowAlpha="0.5" shadowColor="#FF1493" strength="0.5"/>
							</filters>
							<fill>
								<BitmapFill x="0" y="0" scaleX="0.5" scaleY="0.5" rotation="225" source="@Embed('../../../../../../../Assets/Images/greenrect.jpg')"/>
							</fill>
							<stroke>
								<SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
							</stroke>
						</Rect>
					</Group>
				</Group>
				<Group maskType="clip">				
					<mask>				
						<Group x="0" y="380">
							<Ellipse width="100" height="50">
								<fill>
									<SolidColor color="#ff0000"/>
								</fill>
								<transform>
									<Transform>
										<matrix>
											<Matrix a="3" b="-2" c="2" d="3" tx="10" ty="30"/>										
										</matrix>
									</Transform>
								</transform>
							</Ellipse>
						</Group>						
					</mask>	
					
					<Group x="0" y="330">
						<filters>
							<BevelFilter distance="15" highlightAlpha="0.5" highlightColor="#228B22" shadowAlpha="0.5" shadowColor="#FF1493" knockout="true"/>
						</filters>
						<Ellipse width="120" height="80">
							<fill>
								<SolidColor color="#0000FF"/>
							</fill>
						</Ellipse>
					</Group>
					<Group x="230" y="310">
						<Path winding="nonZero" y="30" data="M  72 0   q   -100   50    0   50   Q -28  50   72  100  q   100  -50   0   -50   Q   172  50    72 0 z" >
							<fill>
								<LinearGradient x = "10" y = "10" scaleX = "50" rotation = "-17">
									<GradientEntry color="#203c00" ratio="0" alpha="1"/>
									<GradientEntry color="#1b971b" ratio="0.28" alpha="1"/>
									<GradientEntry color="#1e4f45" ratio="0.45" alpha="1"/>
									<GradientEntry color="#6aec01" ratio="0.6" alpha="1"/>
									<GradientEntry color="#1b971b" ratio="0.75" alpha="1"/>
									<GradientEntry color="#203c00" ratio="1" alpha="1"/>
								</LinearGradient>
							</fill>
							<stroke>
							  <SolidColorStroke color="#00cf00" caps="none" weight="2" joints="miter" miterLimit="4"/>
							</stroke>
						</Path>
					</Group>					
				</Group>
			    </Group>
			</Group>

		</Group>

	    </Group>   
	</Group>
</Graphic>