        <s:fill>
            <s:SolidColor color="#707070"/>
        </s:fill>
    </s:Rect>
    
    <s:Rect top="4" width="16" height="1">
        <s:fill>
            <s:SolidColor color="#707070"/>
        </s:fill>
    </s:Rect>
    
    
</s:Skin>