<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
						xmlns:s="library://ns.adobe.com/flex/spark" 
						>
	
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<!-- background -->
	<s:Rect left="0" right="0" top="0" bottom="0" width="16" height="16" radiusX="2" radiusY="2">
		<s:stroke>
			<s:SolidColorStroke color="0x5380D0"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xFFFFFF" color.over="0xEBF4FF" color.down="0xDEEBFF"/>
		</s:fill>
	</s:Rect>
	
	<!-- shadow (down state only) -->
	<s:Rect left="2" top="1" right="2" height="1" includeIn="down">
		<s:fill>
			<s:SolidColor color="0x5380D0" alpha="0.25" />
		</s:fill>
	</s:Rect>
	<s:Rect left="1" top="2" right="1" height="1" includeIn="down">
		<s:fill>
			<s:SolidColor color="0x5380D0" alpha="0.07" />
		</s:fill>
	</s:Rect>
	<s:Rect left="1" top="2" bottom="1" width="1" includeIn="down">
		<s:fill>
			<s:SolidColor color="0x5380D0" alpha="0.07" />
		</s:fill>
	</s:Rect>
	<s:Rect right="1" top="2" bottom="1" width="1" includeIn="down">
		<s:fill>
			<s:SolidColor color="0x5380D0" alpha="0.07" />
		</s:fill>
	</s:Rect>
	
	<s:Rect width="8" height="2" top="8" left="5">
		<s:fill>
			<s:SolidColor color="0x5581D1"/>
		</s:fill>
	</s:Rect>
	
	<s:Rect width="2" height="8" top="5" left="8">
		<s:fill>
			<s:SolidColor color="0x5581D1"/>
		</s:fill>
	</s:Rect>
	
</s:Skin>